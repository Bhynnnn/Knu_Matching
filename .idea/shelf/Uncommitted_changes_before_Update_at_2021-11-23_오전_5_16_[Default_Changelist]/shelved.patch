+                FileOutputStream fos = new FileOutputStream(file);
+                for (;;) {
+                    Read = is.read(tmpByte);
+                    if (Read <= 0) {
+                        break;
+                    }
+                    fos.write(tmpByte, 0, Read);
+                }
+                is.close();
+                fos.close();
+                conn.disconnect();
